{
  "logging": {
    "debug": false,
    "append": false
  },
  "cache": {
    "path": "/app/trakt_cache"
  },
  "sync": {
    "trakt_to_plex": {
      "watched_status": true,
      "ratings": true,
      "liked_lists": true,
      "watchlist": true
    },
    "plex_to_trakt": {
      "watched_status": true,
      "ratings": true,
      "collection": true
    }
  },
  "watch": {
    "scrobble_threshold": 80,
    "add_collection": false,
    "remove_collection": false,
    "username_filter": false
  },
  "xbmc-providers": {
    "movies": "imdb",
    "shows": "tvdb"
  },
  "excluded-libraries": ["Private", "Family Holidays"]
}
